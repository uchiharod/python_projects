   "source": [
    "import turtle\n",
    "\n",
    "t = turtle.Turtle()\n",
    "t.shape(\"turtle\")\n",
    "t.color(\"green\")\n",
    "t.pensize(3)\n",
    "t.pencolor(\"blue\")\n",
    "t.penup()\n",
    "t.goto(0, -100)\n",
    "t.setheading(180)\n",
    "t.pendown()\n",
    "\n",
    "for i in range(5):\n",
    "    t.forward(200)\n",
    "    t.right(144)\n",
    "\n",
    "turtle.done()"
   ]
